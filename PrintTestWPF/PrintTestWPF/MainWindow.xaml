<Window x:Class="PrintTestWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PrintTestWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="btnPrintPdf" Content="Print .pdf" HorizontalAlignment="Left" Margin="211,64,0,0" VerticalAlignment="Top" Width="75" Click="btnPrintPdf_Click"/>
        <Button x:Name="btnPrintDocx" Content="Print .docx" HorizontalAlignment="Left" Margin="211,111,0,0" VerticalAlignment="Top" Width="75" Click="btnPrintDocx_Click" />

    </Grid>
</Window>
