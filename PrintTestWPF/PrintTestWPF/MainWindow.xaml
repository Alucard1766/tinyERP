<Window x:Class="PrintTestWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PrintTestWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="btnPrintPdf" Content="Print .pdf with Spire.PDF" HorizontalAlignment="Left" Margin="156,64,0,0" VerticalAlignment="Top" Width="178" Click="btnPrintPdf_Click"/>
        <Button x:Name="btnOpenPdf" Content="Open .pdf in Default App" HorizontalAlignment="Left" Margin="156,111,0,0" VerticalAlignment="Top" Width="178" Click="btnOpenPdf_Click" />
        <Button x:Name="btnPrintPdfAdobe" Content="Print .pdf with Adobe" HorizontalAlignment="Left" Margin="156,159,0,0" VerticalAlignment="Top" Width="178" Click="btnPrintPdfAdobe_Click" />
        <Button x:Name="btnOpenDocx" Content="Open .docx in Default App" HorizontalAlignment="Left" Margin="156,201,0,0" VerticalAlignment="Top" Width="178" Click="btnOpenDocx_Click"/>
        <Button x:Name="btnOpenUnknown" Content="Open .unknown in Default App" HorizontalAlignment="Left" Margin="156,243,0,0" VerticalAlignment="Top" Width="178" Click="btnOpenUnknown_Click"/>

    </Grid>
</Window>
